def bubble_sort(arr):
    i=0
    for i in range(len(arr)-1):
        for j in range(len(arr)-i-1):
            if arr[j]>arr[j+1]:
                temp=arr[j]
                arr[j]=arr[j+1]
                arr[j+1]=temp
def display(arr):
    for i in range(len(arr)):
        print(arr[i],end=" ")
    print()
arr=[12,100,33,-10,56,1,-90]
print("before sorting")
display(arr)
bubble_sort(arr)
display(arr)



     
